{
  "expo": {
    "name": "AZHAAN TRADER",
    "slug": "azhaan-trader",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a1a2e"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ecommerce.mobileapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1a1a2e"
      },
      "package": "com.ecommerce.mobileapp"
    },
    "plugins": [
      "expo-secure-store",
      "expo-font",
      "expo-asset"
    ],
    "extra": {
      "eas": {
        "projectId": "4bce945f-aeae-45df-8ac0-744f19473e21"
      }
    },
    "owner": "maaz5624"
  }
}